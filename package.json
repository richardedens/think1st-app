{
    "name": "think1st",
    "version": "0.0.1",
    "preferGlobal": true,
    "description": "The Think1st.app portal.",
    "main": "./dist/src/runtime/src/server",
    "engines": {
        "node": ">=0.9.9"
    },
    "engine-strict": true,
    "scripts": {
        "start": "node ./dist/src/runtime/src/server",
        "test": "npm-run-all run:tsc-test run:ng-test",
        "build": "npm-run-all run:tsc-build run:dbsync run:migration",
        "prod": "npm-run-all run:tsc-build run:dbsync run:migration run:start",
        "prod:db": "npm-run-all run:dbsync run:migration",
        "prod:server": "npm-run-all run:tsc-build run:dbsync run:migration run:start",
        "ng": "ng",
        "run:start": "node ./dist/src/runtime/src/server",
        "run:dependencies": "npm install -g typescript",
        "run:tsc-test": "mocha -r ts-node/register src/runtime/test/**/*.spec.ts",
        "run:tsc-build": "tsc",
        "run:dbsync": "node ./dist/src/runtime/src/dbsync",
        "run:migration": "ts-node ./node_modules/typeorm/cli.js migration:run",
        "typeorm": "ts-node ./node_modules/typeorm/cli.js"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/richardedens/terraformer.git"
    },
    "bugs": {
        "url": "https://github.com/richardedens/terraformer/issues"
    },
    "keywords": [
        "terraformer",
        "webapp",
        "app",
        "low-code",
        "platform"
    ],
    "author": "Gerhard Richard Edens",
    "license": "MIT",
    "homepage": "https://terraformer.app/",
    "nodemonConfig": {
        "restartable": "rs",
        "ignore": [
            "node_modules/**/node_modules"
        ],
        "delay": "2500",
        "env": {
            "NODE_ENV": "development",
            "PORT": 3000
        }
    },
    "dependencies": {
        "@types/bcryptjs": "^2.4.2",
        "@types/body-parser": "^1.19.0",
        "@types/chai": "^4.2.12",
        "@types/connect-ensure-login": "^0.1.5",
        "@types/cookie-parser": "^1.4.2",
        "@types/cors": "^2.8.7",
        "@types/csurf": "^1.9.36",
        "@types/express-session": "^1.17.0",
        "@types/helmet": "0.0.45",
        "@types/http-errors": "^1.8.0",
        "@types/jasmine": "^3.5.12",
        "@types/jasminewd2": "^2.0.8",
        "@types/jsonwebtoken": "^8.5.0",
        "@types/mendixmodelsdk": "^0.8.1",
        "@types/mocha": "^5.2.7",
        "@types/morgan": "^1.9.1",
        "@types/node": "^12.12.54",
        "@types/node-sass-middleware": "0.0.31",
        "@types/office-js": "^1.0.124",
        "@types/passport": "^1.0.4",
        "@types/passport-local": "^1.0.33",
        "@types/protractor": "^4.0.0",
        "async": "^3.2.0",
        "bcryptjs": "^2.4.3",
        "bluebird": "^3.7.2",
        "body-parser": "^1.19.0",
        "bootstrap": "^4.5.2",
        "chalk": "^2.4.2",
        "chart.js": "^2.9.3",
        "class-validator": "^0.10.2",
        "clear": "^0.1.0",
        "compression": "^1.7.4",
        "connect-ensure-login": "^0.1.1",
        "connect-typeorm": "^1.1.4",
        "cookie-parser": "^1.4.5",
        "core-js": "^3.6.5",
        "cors": "^2.8.5",
        "csurf": "^1.11.0",
        "debug": "~4.1.1",
        "dotenv": "^8.2.0",
        "ejs": "^3.1.5",
        "express": "~4.17.3",
        "express-jwt": "^6.0.0",
        "express-session": "^1.17.1",
        "figlet": "^1.5.0",
        "helmet": "^3.23.3",
        "http-errors": "~1.7.3",
        "https": "^1.0.0",
        "jsonwebtoken": "^8.5.1",
        "morgan": "~1.9.1",
        "mssql": "^5.1.2",
        "mysql": "^2.18.1",
        "node-sass-middleware": "0.11.0",
        "npm-run-all": "^4.1.5",
        "open": "^7.1.0",
        "passport": "^0.4.1",
        "passport-local": "^1.0.0",
        "prompt": "^1.0.0",
        "reflect-metadata": "^0.1.13",
        "request": "~2.87.0",
        "rxjs": "^6.6.2",
        "sqlite3": "^4.2.0",
        "tempfile": "^3.0.0",
        "ts-node-dev": "^1.0.0-pre.58",
        "tslib": "^1.13.0",
        "twing": "^4.0.6",
        "typeorm": "^0.2.25",
        "typeorm-encrypted": "^0.5.5",
        "typeorm-static": "^1.1.1",
        "uuid": "^7.0.3",
        "when": "^3.7.8",
        "zone.js": "~0.9.1"
    },
    "devDependencies": {
        "chai": "^4.2.0",
        "codelyzer": "^5.2.2",
        "eslint": "^6.8.0",
        "eslint-config-airbnb-base": "^14.2.0",
        "eslint-plugin-import": "^2.22.0",
        "jasmine-core": "~3.4.0",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "^5.1.1",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "^2.1.1",
        "karma-jasmine": "~2.0.1",
        "karma-jasmine-html-reporter": "^1.5.4",
        "mocha": "^8.1.1",
        "protractor": "^7.0.0",
        "ts-node": "^8.10.2",
        "tslint": "~5.19.0",
        "typescript": "^3.9.7"
    }
}
