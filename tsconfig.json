{
  "compilerOptions": {
      "types": ["mocha", "chai", "jasmine"],
      "lib": [
        "es5",
        "es6"
     ],
     "target": "es5",
     "module": "commonjs",
     "moduleResolution": "node",
     "outDir": "./dist",
     "emitDecoratorMetadata": true,
     "experimentalDecorators": true,
     "resolveJsonModule": true,
     "sourceMap": true,
     "skipLibCheck": true,
     "esModuleInterop": true,
     "baseUrl": ".",
     "paths": {
         "*": [
            "node_modules/*",
            "src/types/*"
         ]
      }
  },
  "include": [
      "./src/frontend/app/e2e/src/protractor.config.ts",
      "src/**/*"
  ]
}